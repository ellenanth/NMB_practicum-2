clear;
x = [1;2;3];
y = [1;2;3];
F = [1 2 1;2 3 2;1 2 1];
m = 3;
n = 3;
C = kkb(x,y,F,m,n);

x_s = [];
y_s = [];
F_s = [];
for i=1:size(x,1)
    for j=1:size(y,1)
        x_s = [x_s; x(i)];
        y_s = [y_s; y(j)];
        F_s = [F_s; F(i,j)];
    end
end

F_new = zeros(3,3);
x = linspace(1,3,30);
y = linspace(1,3,30);
for i = 1:size(x,1)
    for j = 1:size(y,1)
        F_new(i,j) = kkb_evaluate(C,x(i),y(j));
    end
end
surf(x,y,F_new);
scatter3(x_s,y_s,F_s, 50, 'filled');
